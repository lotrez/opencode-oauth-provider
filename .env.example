# OAuth Configuration for opencode-oauth-provider
# Copy this to .env and fill in your values

# OAuth Token Endpoint (required)
OAUTH_TOKEN_URL=http://localhost:8787/oauth/token

# OAuth Client Credentials (required)
OAUTH_CLIENT_ID=test-client-id
OAUTH_CLIENT_SECRET=test-client-secret

# OAuth Scope (optional)
OAUTH_SCOPE=openai

# Target host to intercept (optional - if not set, intercepts all LLM API calls)
OAUTH_TARGET_HOST=localhost:8787
